@{
    ViewData["Title"] = "Display Data";
    string[] TableHeaders = new string[] { "Home", "Classes", "Professors", "Tutors", "Textbook Exchange" };
}
<html>
<head>

</head>
<body style="background-color:#debab1">
    <div class="table">
        <table class="table table-bordered table-hover">
            <thead>
                <tr>
                    @{
                        foreach (var head in TableHeaders)
                        {
                            <th>
                                @head
                            </th>
                        }
                    }
                </tr>
            </thead>
            <tbody>
                @{
                    if (Model != null)
                    {
                        foreach (var Data in Model)
                        {
                            <tr>
                                <td>@Data.Class_Title</td>
                                <td>@Data.Class_Code</td>
                                <td>@Data.Class_Description</td>
                            </tr>


                        }
                    }
                }
            </tbody>
        </table>
    </div>
</body>
</html>
